{
  "name": "electron-app",
  "productName": "electron-app",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".vite/build/main_electron.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "build": "npx vue-tsc --noEmit && npx vite build --config vite.standalone.config.mts",
    "dev": "npx vite --config vite.standalone.config.mts",
    "pretty:watch": "npx onchange \"**/*.*{ts,vue,svelte,tsx,css,scss,html}\" -- npx prettier --config .prettierrc --write --ignore-unknown {{changed}}",
    "pretty": "npx prettier --config .prettierrc --loglevel warn -w \"**/*.*{ts,vue,svelte,tsx,css,scss,html}\""
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron-forge/plugin-vite": "^7.4.0",
    "@electron/fuses": "^1.8.0",
    "@types/node": "^20.14.11",
    "@vitejs/plugin-vue": "^5.0.5",
    "autoprefixer": "^10.4.19",
    "electron": "31.2.1",
    "husky": "^9.1.1",
    "onchange": "^7.1.0",
    "postcss": "^8.4.39",
    "prettier": "^3.3.3",
    "pretty-quick": "^4.0.0",
    "tailwindcss": "^3.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "vite": "^5.3.4",
    "vitest": "^2.0.3",
    "vue-tsc": "^2.0.26"
  },
  "keywords": [],
  "author": {
    "name": "damianof",
    "email": "damianof@users.noreply.github.com"
  },
  "license": "MIT",
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "vue": "^3.4.33"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged \"**/*.*{ts,vue,svelte,tsx,css,scss,html}\""
    }
  }
}
